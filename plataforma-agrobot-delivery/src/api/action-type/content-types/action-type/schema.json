{
  "kind": "collectionType",
  "collectionName": "action_types",
  "info": {
    "singularName": "action-type",
    "pluralName": "action-types",
    "displayName": "ActionType"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "actionName": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "minLength": 1,
      "unique": true
    },
    "actionDescription": {
      "type": "string",
      "required": false,
      "maxLength": 255,
      "minLength": 1
    },
    "actions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::action.action",
      "mappedBy": "action_type"
    }
  }
}
