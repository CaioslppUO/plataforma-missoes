{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "longitude": {
      "type": "float",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "locationOrder": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "mission": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mission.mission",
      "inversedBy": "locations"
    },
    "actions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::action.action",
      "mappedBy": "location"
    }
  }
}
